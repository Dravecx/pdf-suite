{
    "name": "PDF Document",
    "module": "PDF Suite",
    "doctype": "DocType",
    "engine": "InnoDB",
    "is_submittable": 0,
    "istable": 0,
    "issingle": 0,
    "editable_grid": 1,
    "track_changes": 0,
    "autoname": "hash",
    "fields": [
        {
            "fieldname": "source_file",
            "fieldtype": "Data",
            "label": "Source File",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "annotations",
            "fieldtype": "Long Text",
            "label": "Annotations",
            "description": "JSON array of Fabric.js annotation objects per page"
        },
        {
            "fieldname": "page_modifications",
            "fieldtype": "Long Text",
            "label": "Page Modifications",
            "description": "JSON array of page operations (reorder, delete, rotate)"
        },
        {
            "fieldname": "output_file",
            "fieldtype": "Data",
            "label": "Output File",
            "description": "URL of the exported PDF"
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        },
        {
            "role": "All",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 0,
            "if_owner": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC"
}
